    <style name="Base.TextAppearance.AppCompat.Medium.Inverse" parent="@*android:style/TextAppearance.Material.Medium" />
    <style name="Base.TextAppearance.AppCompat.Small.Inverse" parent="@*android:style/TextAppearance.Material.Small" />
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.Material">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffffffff</item>
        <item name="android:textColorSecondaryInverse">#ffffffff</item>
        <item name="android:textColorSecondaryInverseNoDisable">#ffffffff</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textColorTertiary">#ffffffff</item>
        <item name="android:textColorTertiaryInverse">#ffffffff</item>
        <item name="android:editTextColor">#ffffffff</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="NotificationPrimaryText">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="NotificationSecondaryText">
        <item name="android:textColor">#ffdddddd</item>
    </style>
    <style name="UnifiedEmailTheme" parent="@*android:style/Theme.Material">
        <item name="android:listViewWhiteStyle">@*android:style/Widget.Material.ListView</item>
        <item name="android:background">@*android:color/background_dark</item>
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="GmailTheme.NoActionBar.WelcomeTour" parent="@style/GmailTheme.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/background_dark</item>
    </style>
    <style name="LabelSyncTheme" parent="@*android:style/Theme.Material.Dialog" />
    <style name="GmailTheme.NoActionBar" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:colorPrimary">#ffd32f2f</item>
        <item name="android:colorPrimaryDark">#ffb71c1c</item>
        <item name="android:colorAccent">#ff0288d1</item>
    </style>
    <style name="GmailTheme.NoActionBar.Fullscreen" parent="@*android:style/Theme.Material.NoActionBar.Fullscreen">
        <item name="android:colorPrimary">#ffd32f2f</item>
        <item name="android:colorPrimaryDark">#ffb71c1c</item>
        <item name="android:colorAccent">#ff0288d1</item>
    </style>

    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>

    <style name="MinTimeProgressViewStyle" parent="@*android:style/Widget.Material.ProgressBar" />
    <style name="MinTimeProgressDialogStyleBase" parent="@*android:style/Theme.Material.Dialog" />
    <style name="ConversationListFadeBase" parent="@*android:style/Widget.Material.ListView" />

    <style name="MailActivityTheme" parent="@*com.google.android.gm:style/Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="*com.google.android.gm:windowActionModeOverlay">true</item>
        <item name="android:colorPrimary">@*com.google.android.gm:color/primary_color</item>
        <item name="android:colorPrimaryDark">@*com.google.android.gm:color/primary_dark_color</item>
        <item name="android:colorAccent">@*com.google.android.gm:color/accent_blue</item>
    </style>
