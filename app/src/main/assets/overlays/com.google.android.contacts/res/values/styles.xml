<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearanceMedium" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearanceSmall" parent="@*android:style/TextAppearance.Small">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
    </style>
    <style name="ListViewStyle" parent="@*android:style/Widget.Material.ListView">
        <item name="android:overScrollMode">always</item>
    </style>
    <style name="BackgroundOnlyTheme" parent="@*android:style/Theme.Material">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoDisplay">false</item>
    </style>
    <style name="Theme.CallSubjectDialogTheme" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">false</item>
        <item name="android:windowElevation">0.0dip</item>
    </style>
    <style name="DetailActivityTheme" parent="@*android:style/Theme.Holo">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffdddddd</item>
        <item name="android:listViewStyle">@style/ListViewStyle</item>
    </style>
    <style name="ContactsActionBarTabTextStyle" parent="@*android:style/Widget.Material.ActionBar.TabText">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="ContactsActionBarDropDownStyle" parent="@*android:style/Widget.Holo.Spinner" />
    <style name="ContactsActionBarTheme" parent="@*android:style/Theme.Holo">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:popupMenuStyle">@*android:style/Widget.Holo.PopupMenu</item>
    </style>
    <style name="NonPhoneDialogTheme" parent="@*android:style/Theme.Material.Dialog" />
    <style name="ConfirmAddDetailDialogTheme" parent="@*android:style/Theme.Material.Dialog.MinWidth">
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="ContactEditorAccountsChangedActivityTheme" parent="@*android:style/Theme.Material.Dialog.NoActionBar.MinWidth">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffdddddd</item>
        <item name="android:listViewStyle">@*com.google.android.contacts:style/ListViewStyle</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="SectionDivider">
        <item name="android:background">#00000000</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0dip</item>
    </style>
    <style name="SelectableItem" parent="@*android:style/Theme.Material">
        <item name="android:background">?android:selectableItemBackground</item>
    </style>
    <style name="Theme.PhotoSelector" parent="@*android:style/Theme.Holo">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="ContactsAlertDialogTheme" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="AccountTypeNameStyle">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="PeopleTheme1" parent="@*android:style/Theme.Material">
        <item name="android:colorPrimary">#ff1976d2</item>
        <item name="android:colorPrimaryDark">#ff0d47a1</item>
        <item name="android:colorAccent">#ff1976d2</item>
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
    </style>


    <style name="EditKindTextAppearanceStyle" parent="@*android:style/TextAppearance.Small">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="EmptyStateTextStyle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:alpha">0.54</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="SingleLineTextStyle">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>


    <style name="PopupMenuStyle" parent="@*android:style/Widget.PopupMenu">
        <item name="android:popupBackground">@*android:color/background_floating_material_dark</item>
    </style>
    <style name="BaseTextStyle">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>



    <style name="AssistantCardButtonStyle" parent="@*android:style/Widget.Material.Button.Borderless.Colored">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:padding">@*com.google.android.contacts:dimen/assistant_card_button_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.Material" />



    <style name="LaunchScreenTheme" parent="@*com.google.android.contacts:style/Theme.AppCompat">
        <item name="android:windowBackground">@*com.google.android.contacts:drawable/quantum_launchscreen_contacts</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
    </style>
    <style name="OnboardingSignInTheme" parent="@*com.google.android.contacts:style/Theme.AppCompat">
        <item name="android:windowBackground">@*android:color/background_dark</item>
        <item name="android:colorPrimaryDark">@*android:color/primary_dark_material_dark</item>
        <item name="*com.google.android.contacts:windowActionBar">false</item>
        <item name="*com.google.android.contacts:windowNoTitle">true</item>
    </style>

    <style name="Dialpad_Light">
        <item name="*com.google.android.contacts:dialpad_text_color">@*com.google.android.contacts:color/dialpad_digits_text_color</item>
        <item name="*com.google.android.contacts:dialpad_text_color_primary">@*com.google.android.contacts:color/dialpad_primary_text_color</item>
        <item name="*com.google.android.contacts:dialpad_text_color_secondary">@*com.google.android.contacts:color/dialpad_secondary_text_color</item>
        <item name="*com.google.android.contacts:dialpad_icon_tint">@*com.google.android.contacts:color/dialpad_icon_tint</item>
        <item name="*com.google.android.contacts:dialpad_voicemail_tint">@*com.google.android.contacts:color/dialpad_voicemail_tint</item>
        <item name="*com.google.android.contacts:dialpad_background">@*com.google.android.contacts:color/background_dialpad</item>
        <item name="*com.google.android.contacts:dialpad_separator_color">#00000000</item>
        <item name="*com.google.android.contacts:dialpad_margin_style">@*com.google.android.contacts:style/DialpadSpaceStyle</item>
        <item name="*com.google.android.contacts:dialpad_delete_padding">@*com.google.android.contacts:dimen/dialpad_digits_padding</item>
    </style>
    <style name="Dialpad_LightTransparent">
        <item name="*com.google.android.contacts:dialpad_text_color">?android:textColorPrimary</item>
        <item name="*com.google.android.contacts:dialpad_text_color_primary">?android:textColorPrimary</item>
        <item name="*com.google.android.contacts:dialpad_text_color_secondary">?android:textColorSecondary</item>
        <item name="*com.google.android.contacts:dialpad_icon_tint">@*com.google.android.contacts:color/dialpad_icon_tint_dark</item>
        <item name="*com.google.android.contacts:dialpad_voicemail_tint">@*com.google.android.contacts:color/dialpad_icon_tint_dark</item>
        <item name="*com.google.android.contacts:dialpad_background">@*android:color/transparent</item>
        <item name="*com.google.android.contacts:dialpad_separator_color">@*android:color/transparent</item>
        <item name="*com.google.android.contacts:dialpad_margin_style">@*com.google.android.contacts:style/DialpadNoSpaceStyle</item>
        <item name="*com.google.android.contacts:dialpad_delete_padding">1.0dip</item>
    </style>
</resources>
